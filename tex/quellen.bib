@INPROCEEDINGS{Zhao:2011:GCW:1989323.1989413,
  author = {Zhao, Peixiang and Li, Xiaolei and Xin, Dong and Han, Jiawei},
  title = {Graph cube: on warehousing and OLAP multidimensional networks},
  booktitle = {Proceedings of the 2011 ACM SIGMOD International Conference on Management
	of data},
  year = {2011},
  series = {SIGMOD '11},
  pages = {853--864},
  address = {New York, NY, USA},
  publisher = {ACM},
  __markedentry = {[s1ck:2]},
  abstract = {We consider extending decision support facilities toward large sophisticated
	networks, upon which multidimensional attributes are associated with
	network entities, thereby forming the so-called multidimensional
	networks. Data warehouses and OLAP (Online Analytical Processing)
	technology have proven to be effective tools for decision support
	on relational data. However, they are not well-equipped to handle
	the new yet important multidimensional networks. In this paper, we
	introduce Graph Cube, a new data warehousing model that supports
	OLAP queries effectively on large multidimensional networks. By taking
	account of both attribute aggregation and structure summarization
	of the networks, Graph Cube goes beyond the traditional data cube
	model involved solely with numeric value based group-by's, thus resulting
	in a more insightful and structure-enriched aggregate network within
	every possible multidimensional space. Besides traditional cuboid
	queries, a new class of OLAP queries, crossboid, is introduced that
	is uniquely useful in multidimensional networks and has not been
	studied before. We implement Graph Cube by combining special characteristics
	of multidimensional networks with the existing well-studied data
	cube techniques. We perform extensive experimental studies on a series
	of real world data sets and Graph Cube is shown to be a powerful
	and efficient tool for decision support on large multidimensional
	networks.},
  acmid = {1989413},
  doi = {10.1145/1989323.1989413},
  file = {:Quellen/Related Work/sigmod11_pzhao.pdf:PDF},
  isbn = {978-1-4503-0661-4},
  keywords = {OLAP, data cube, data warehouse, graph cube, multidimensional network},
  location = {Athens, Greece},
  numpages = {12},
  review = {- gute Abgrenzung zum klassischen Cube bei dem nur Dimensionen und
	Measures eine Rolle spielen und keine Beziehungen zwischen Entitäten
	
	
	Multidimensional networks = Property Graph
	
	- Dimensionen sind die Anzahl verschiedener Properties an Knoten!
	(Kanten werden nicht berücksichtigt)
	
	
	- Bilden von Aggregierten Graphen auf Basis gemeinsamer Knoteneigenschaften
	(cuboide)
	
	- Join von Aggregierten Graphen (crossboide)
	
	
	- partielle Materialisierung (auf Basis des heuristischen Algorithmus
	aus DWH)},
  url = {http://doi.acm.org/10.1145/1989323.1989413}
}

